<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".menu.Social.Party.PartyMenu">

    <TextView
        android:id="@+id/partyNameValue"
        android:layout_width="208dp"
        android:layout_height="20dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="15dp"
        android:text="Party Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.163"
        app:layout_constraintStart_toEndOf="@+id/partyNameText"
        app:layout_constraintTop_toBottomOf="@+id/partyMenuTitle" />

    <TextView
        android:id="@+id/partyDescriptionValue"
        android:layout_width="206dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:text="description..."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/partyNameValue"
        app:layout_constraintTop_toBottomOf="@+id/partyNameValue" />

    <Button
        android:id="@+id/partyDeleteButton"
        android:layout_width="93dp"
        android:layout_height="41dp"
        android:layout_marginEnd="140dp"
        android:text="Abandon"
        app:layout_constraintEnd_toStartOf="@+id/partyGenerateButton"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/partyGenerateButton" />

    <Button
        android:id="@+id/partyCreateButton"
        android:layout_width="95dp"
        android:layout_height="40dp"
        android:layout_marginTop="11dp"
        android:layout_marginBottom="92dp"
        android:text="Create"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/guildGenerateButton"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/partyGenerateButton"
        app:layout_constraintTop_toBottomOf="@+id/guildGenerateButton"
        app:layout_constraintVertical_bias="0.033" />

    <Button
        android:id="@+id/partyJoinButton"
        android:layout_width="93dp"
        android:layout_height="41dp"
        android:layout_marginTop="9dp"
        android:text="Join"
        app:layout_constraintBottom_toBottomOf="@+id/partyCreateButton"
        app:layout_constraintEnd_toEndOf="@+id/partyDeleteButton"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/partyDeleteButton"
        app:layout_constraintTop_toBottomOf="@+id/partyDeleteButton"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/partyGenerateButton"
        android:layout_width="95dp"
        android:layout_height="40dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="40dp"
        android:text="Generate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout2" />

    <TextView
        android:id="@+id/partyDescriptionText"
        android:layout_width="84dp"
        android:layout_height="23dp"
        android:layout_marginTop="8dp"
        android:text="Description:"
        app:layout_constraintBottom_toTopOf="@+id/relativeLayout2"
        app:layout_constraintEnd_toEndOf="@+id/partyNameText"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/partyNameText"
        app:layout_constraintTop_toBottomOf="@+id/partyNameText"
        app:layout_constraintVertical_bias="0.024" />

    <TextView
        android:id="@+id/partyNameText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Party Name:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/partyNameValue" />

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="409dp"
        android:layout_height="303dp"
        android:layout_marginTop="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/partyDescriptionValue">

        <ListView
            android:id="@+id/listView"
            android:layout_width="407dp"
            android:layout_height="479dp" />
    </RelativeLayout>

    <TextView
        android:id="@+id/partyMenuTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="16dp"
        android:text="Party Menu"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>